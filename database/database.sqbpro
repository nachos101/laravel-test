-- Contar álbumes por artista
SELECT 
    artist,
    COUNT(*) as total_albums,
    MIN(release_date) as primer_album,
    MAX(release_date) as ultimo_album
FROM albums 
WHERE artist IN ('Twenty One Pilots', 'Airbag')
GROUP BY artist;

-- Ver todos los álbumes ordenados por artista y fecha
SELECT 
    id,
    name,
    artist,
    genre,
    release_date,
    price
FROM albums 
WHERE artist IN ('Twenty One Pilots', 'Airbag')
ORDER BY artist, release_date;</sql><current_tab id="0"/></tab_sql></sqlb_project>
